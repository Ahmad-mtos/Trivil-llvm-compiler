модуль factorial
импорт "стд::llvm"


вход { 
    пусть n : Цел64 := 10
    пусть factorial : Цел64 := 1
    пока (истина) {
        если n = 0 {
            прервать
        }
        иначе {
            factorial := factorial * n 
            n--
        }
    }
    //llvm.цел64(factorial)
}

