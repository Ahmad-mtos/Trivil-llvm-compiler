модуль euclidean_gcd
//импорт "стд::llvm"

пусть a: Цел64 := 13040
пусть b: Цел64 := 542890

фн euclidean GCD (n : Цел64, m : Цел64) : Цел64 {
    если n > m {
        пусть temp: Цел64 := n 
        n := m
        m := temp
    }

    пока (истина) {
        если n = 0 {
            вернуть m
        }
        иначе {
            пусть temp: Цел64 := n 
            n := m % n
            m := temp
        }
    }
    вернуть 1
}

вход {
    //llvm.цел64(euclidean GCD(a, b))
}